{
  "version": 2,
  "builds": [
    {
      "src": "api/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
      
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/example/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "example/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/wsgi.py"
    }
  ]
}
